{
	"$schema": "https://turborepo.org/schema.json",
	"tasks": {
		"build": {
			"dependsOn": ["^build"]
		},
		"//#dev:setup": {
			"cache": false
		},
		"//#dev:start": {
			"cache": false,
			"dependsOn": ["//#dev:setup"],
			"persistent": true
		},
		"dev": {
			"dependsOn": ["//#dev:setup"],
			"cache": false,
			"persistent": true
		}
	}
}
